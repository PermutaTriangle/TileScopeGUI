[MASTER]
ignore-patterns=test_.*?py
ignore=tests
init-hook="from pylint.config import find_pylintrc; import os, sys; sys.path.append(os.path.dirname(find_pylintrc())+'/tilescopegui')"
disable=bad-continuation,
        missing-module-docstring,
        fixme,
        unsubscriptable-object,
good-names=i,j,n,k,x,y,f,_

[SIMILARITIES]
ignore-imports=yes
